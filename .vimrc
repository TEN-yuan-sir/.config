syntax on
set mouse=a
set number
set autowrite
set showmatch
set incsearch
set cursorline
set ruler
set noeb
set cindent 
set tabstop=4
set shiftwidth=4
set noexpandtab
set nobackup
set noswapfile
set hlsearch
set gdefault

autocmd bufnewfile * startinset

imap jk <esc>
nmap Q :q<cr>
nmap S :w<cr>




